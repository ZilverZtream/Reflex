{
  "name": "@reflex/compiler",
  "version": "1.0.0",
  "description": "AOT compiler for Reflex framework - transforms templates to optimized JavaScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite-plugin.mjs",
      "require": "./dist/vite-plugin.cjs",
      "types": "./dist/vite-plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/vite-plugin.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/vite-plugin.ts --format cjs,esm --dts --watch"
  },
  "keywords": [
    "reflex",
    "compiler",
    "aot",
    "template",
    "optimization",
    "vite"
  ],
  "author": "Reflex Team",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "magic-string": "^0.30.5"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
